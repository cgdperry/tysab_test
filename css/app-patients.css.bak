.bouncetext{
	position:absolute;
	top:10px;
	left:10px;
	opacity:1;
	overflow:hidden;
	transition: opacity 1s, height 1s, top 1s, height 1s;
	-webkit-transition: opacity 1s, height 1s, top 1s, height 1s;
}

.bouncetext .bottom{
	position:absolute;
	bottom:0px;
	left:0px;
	width:inherit;
	margin:0;
	padding:0;
	opacity:0.5;
	background-position:top left;
	background-repeat:no-repeat;
	transition: opacity 1s, height 1s;
	-webkit-transition: opacity 1s, height 1s;
}

.bouncetext .top{
	position:absolute;
	top:0px;
	left:0px;
	width:inherit;
	margin:0;
	padding:0;
	opacity:1;
	background-position:top left;
	background-repeat:no-repeat;
	transition: opacity 1s, height 1s;
	-webkit-transition: opacity 1s, height 1s;
}

.bouncetext .arrow, .floattext .arrow{
	top:10px;
	left:0px;
	height:25px;
	width:25px;
	position:absolute;
	background-image:url(../app-patients/images/text_arrow.png);
	background-repeat:no-repeat;
}

.bouncetext.up .top .arrow, .bouncetext.down .bottom .arrow{
	background-position:top right;
}

.bouncetext.down .top .arrow, .bouncetext.up .bottom. arrow, .floattext .arrow {
	background-position:top left;
}

.floattext {
	position:absolute;
	background-repeat:no-repeat;
	background-position:top-left;
}

